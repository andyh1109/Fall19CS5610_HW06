<h1>Show Job</h1>

<ul>

  <li>
    <strong>Job code:</strong>
    <%= @job.job_code %>
  </li>

  <li>
    <strong>Budget:</strong>
    <%= @job.budget %>
  </li>

  <li>
    <strong>Desc:</strong>
    <%= @job.desc %>
  </li>

  <li>
    <strong>Manager:</strong>
    <%= @job.manager_id %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.job_path(@conn, :edit, @job) %></span>
<span><%= link "Back", to: Routes.job_path(@conn, :index) %></span>
